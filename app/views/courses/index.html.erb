<h1><%= t 'assign_courses' %></h1>

<table class="table">
  <thead>
    <th><%= t 'name' %></th>
    <th><%= t 'description' %></th>
  </thead>
  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= link_to course.name, '#' %></td>
        <td><%= course.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>
